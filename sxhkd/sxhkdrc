# Lecture note manager

super + t
    python ~/tools/tex-manag/scripts/dmenu-courses.py

super + n
    python ~/tools/tex-manag/scripts/dmenu-lectures.py

super + r
    python ~/tools/tex-manag/scripts/dmenu-lectures-view.py


###############################

# Volume control
XF86AudioLowerVolume
    pulsemixer --change-volume -5

XF86AudioRaiseVolume
    pulsemixer --change-volume +5

XF86AudioMute
    pulsemixer --toggle-mute
  

# Brightness up
XF86MonBrightnessUp
    brightnessctl s +5%

# And brightness down
XF86MonBrightnessDown
    brightnessctl s 5%-

# Media controls.
XF86Audio{Play,Prev,Next,Stop}
    playerctl {play-pause,previous,next,stop}

